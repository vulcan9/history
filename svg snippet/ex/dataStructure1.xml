<root>
    <doc>
	    <!--hidden infomation-->
		<info>
		    시간, 타이틀, ID, Group(카테고리), 태그(검색, 필터링), domPath
		</info>
		<geo>
		    depth, transition, transform(scale, x, y, rotate, matrix....)
		</geo>

		<!--visual object-->
		<svg>
			<text></text>
			<shape></shape>
			<component></component>
			<doc>
				......
			</doc>
		</svg>
		......
	</doc>
</root>

		// 시간 통계
		"start" : 시작 체크 시간
		"end" : 종료 예상(마감) 시간
		"complete" :  완료 시간



impressJS 를 수정해야 하겠습니다.~!!!




"root-document":{
	"id": rootID,
	"thumbnail":"base64 data",

	"config" : {
		"editable" : 프리젠테이션 or Edit
		"lock" : 문서 잠금 상태 (편집, 열람 불가)
		"permition" :  접근 권한 등급 또는 편집 가능 범위 표시
		//"complete" : 문서 수정 완료 여부 (더이상 수정하지 않음, admin 설정)
	}

	"time": {
		"create" : 최초 생성된 시간,
		"modify" : 마지막으로 수정된 시간
	}

	"dom":{
		group : 카테고리,
		path : 런타임 성능을 위해 필요함 (dom path)
		//parent : dom parent ?
	}

	"info":{
		"title" : 제목,
		"tag" : 태그(검색, 필터링), 
	},
	
	// 구체적인 구현 형식 필요함
	"geometry":{
		"depth" : 0 (문서의 심도),
		"scale" : 상대적인 스케일 값?
		
		//transition, transform(scale, x, y, rotate, matrix....)
		width, height, x, y (parent에 기술되어 있음)

		"boundary" : {
			"actual" : {x,y,w,h} - 스케일 적용되지 않은 정사이즈 rectangle
			"standard" : {x,y,w,h} - 스케일 적용되지 않은 표준 용지 규격
			//"scaledNormal" : {x,y,w,h} - 스케일이 적용된 actual boundary
		}
	},

	"content":{
		'<svg>
			<text></text>
			<shape></shape>
			<component></component>
			...
			<document source="docunent-ID1"></document>
			<document source="docunent-ID2"></document>
			<document source="docunent-ID3"></document>
			<document source="docunent-ID4"></document>
		</svg>'
	},

	"documents":[
		{
			"id": docunent-ID1
			"thumbnail":"base64 data",
			...
		},
		{
			"id": docunent-ID2
			"thumbnail":"base64 data",
			...
		},
		{
			"id": docunent-ID3
			"thumbnail":"base64 data",
			...
		},
		{
			"id": docunent-ID4
			"thumbnail":"base64 data",
			"info":{
				시간, 타이틀, ID, Group(카테고리), 태그(검색, 필터링), domPath
			},
			"geo":{
				depth, transition, transform(scale, x, y, rotate, matrix....)
			},
			"content":{
				'<svg>
					<text></text>
					<shape></shape>
					<component></component>
					...
					<doc source="subDocunent-ID1"></doc>
					<doc source="subDocunent-ID2"></doc>
					<doc source="subDocunent-ID3"></doc>
					<doc source="subDocunent-ID4"></doc>
				</svg>'
			},
			"documents":[
				{
					"id": subDocunent-ID1,
					"thumbnail":"base64 data",
					...
				},
				{
					"id": subDocunent-ID2,
					"thumbnail":"base64 data",
					...
				},
				...
			]
		}
	]
}

